{
  "name": "check",
  "version": "0.0.1",
  "description": "check variables",
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --config ./build/webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config ./build/webpack.config.js",
    "prepare": "NODE_ENV=production webpack --config ./build/webpack.config.js",
    "test": "npm run build:prod && ava"
  },
  "keywords": [
    "check",
    "variables"
  ],
  "author": "sashmlv",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "exports-loader": "^1.1.0",
    "imports-loader": "^1.1.0",
    "jsdom": "^16.2.2",
    "webpack": "^5.0.0-beta.21",
    "webpack-cli": "^3.3.12"
  },
  "ava": {
    "files": [
      "!*.js",
      "**/*.test.js"
    ],
    "verbose": true,
    "timeout": "5s",
    "failFast": true,
    "serial": true
  }
}
