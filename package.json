{
  "name": "check",
  "version": "0.0.1",
  "description": "check variables",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "NODE_ENV=production rollup --config ./build/rollup.config.js",
    "build:dev": "NODE_ENV=development rollup --config ./build/rollup.config.js",
    "build:prod": "NODE_ENV=production rollup --config ./build/rollup.config.js",
    "test": "npm run build:prod && ava"
  },
  "keywords": [
    "check",
    "variables"
  ],
  "author": "sashmlv",
  "license": "ISC",
  "devDependencies": {
    "ava": "^3.9.0",
    "jsdom": "^16.2.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "ava": {
    "files": [
      "!*.js",
      "**/*.test.js"
    ],
    "verbose": true,
    "timeout": "5s",
    "failFast": true,
    "serial": true
  }
}
